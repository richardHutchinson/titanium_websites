var quotes = [
	"a.",
	"b.",
	"c."
];

var counter = 0;

var getPrev = function() {
	//counting backwards
	if(counter == 0) {
		counter = (quotes.length - 1);
		//can console the loop around of the previous value from the quotes array[] - ex: 2,1,0 then previous
	}else {
		counter--;
		//can console the previouse value here - console.log(quotes[counter]); 
	}
};